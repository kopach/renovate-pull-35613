{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  dependencyDashboard: true,
  packageRules: [
    {
      matchDepTypes: ['action'],
      digest: {
        prBodyDefinitions: {
          Change: '[{{#if displayFrom}}{{{displayFrom}}}{{else}}{{{currentValue}}}{{/if}} -> {{#if displayTo}}{{{displayTo}}}{{else}}{{{newValue}}}{{/if}}]({{#if sourceUrl}}{{{sourceUrl}}}/compare/{{{currentDigestShort}}}...{{{newDigestShort}}}{{else}}https://github.com/{{depName}}/compare/{{{currentDigestShort}}}...{{{newDigestShort}}}{{/if}})',
        },
      },
      prBodyDefinitions: {
        Change: '[{{#if displayFrom}}{{{displayFrom}}}{{else}}{{{currentValue}}}{{/if}} -> {{#if displayTo}}{{{displayTo}}}{{else}}{{{newValue}}}{{/if}}]({{#if sourceUrl}}{{{sourceUrl}}}/compare/{{{currentValue}}}...{{{newValue}}}{{else}}https://github.com/{{depName}}/compare/{{{currentValue}}}...{{{newValue}}}{{/if}})',
      },
    },
  ],
}
